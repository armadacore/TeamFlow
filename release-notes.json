{
	"categories": [
		{
			"key": "fixes",
			"title": "## 🐛 Fixes",
			"labels": ["fix", "bug"]
		},
		{
			"key": "features",
			"title": "## 🚀 Features",
			"labels": ["feature", "feat"]
		},
		{
			"key": "tests",
			"title": "## 🧪 Tests",
			"labels": ["test"]
		},
		{
			"key": "documentations",
			"title": "## 📚 Documentations",
			"labels": ["documentation", "doc"]
		}
	],
	"ignore_labels": [
		"ignore",
		"release"
	],
	"template": "#{{CHANGELOG}}",
	"pr_template": "- ##{{NUMBER}} - #{{TITLE}}",
	"max_tags_to_fetch": 200,
	"max_pull_requests": 200,
	"max_back_track_time_days": 365,
	"base_branches": [
		"develop"
	]
}
