{
	"name": "teamflow",
	"version": "0.1.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"vite:dev": "vite",
		"vite:build": "tsc && vite build",
		"tauri": "tauri",
		"tauri:dev": "tauri dev",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"test:ui": "vitest --ui",
		"test:coverage:ui": "vitest --coverage --ui",
		"eslint:check": "eslint ./src && tsc --project tsconfig.json --noEmit",
		"type:check": "tsc --noEmit",
		"license:check": "npx jsgl --dev false --verbose true --local ./",
		"outdated:check": "check-outdated --ignore-pre-releases --ignore-packages @tauri-apps/api --columns name,type,current,latest,changes --types major,minor,reverted"
	},
	"dependencies": {
		"@armadacore/fnt": "^0.4.1",
		"@tauri-apps/api": "2.0.1",
		"@tauri-apps/plugin-shell": "2.2.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@tauri-apps/cli": "2.1.0",
		"@types/react": "^19.0.2",
		"@types/react-dom": "^19.0.2",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/coverage-istanbul": "^2.1.8",
		"@vitest/ui": "^2.1.8",
		"check-outdated": "^2.12.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.37.3",
		"globals": "^15.14.0",
		"js-green-licenses": "^4.0.0",
		"jsdom": "^25.0.1",
		"prettier": "^3.4.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.19.0",
		"vite": "^6.0.6",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^2.1.8"
	}
}
